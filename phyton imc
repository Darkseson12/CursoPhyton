def nivel_de_peso(p_imc):
    if p_imc < 18.5:
        print("Bajo peso")
    elif p_imc >= 18.5 and p_imc <= 24.9:
        print("Normal")
    elif p_imc >= 25.5 and p_imc <= 24.9:
        print("Sobrepeso")
    else:
        print("Obesidad")
        
nivel_de_peso(persona_imc)
    
